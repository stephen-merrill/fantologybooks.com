{
  "name": "fantologybooks.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/styles/sass/bulma.scss src/styles/css/bulma.css",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "stage": "yarn build && aws s3 sync ./dist s3://staging.fantologybooks.com --delete",
    "deploy": "yarn build && aws s3 sync ./dist s3://fantologybooks.com --delete"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "fuse.js": "^3.4.6",
    "imagemin-webpack-plugin": "^2.4.2",
    "lodash.debounce": "^4.0.8",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-infinite-scroll": "^2.0.2",
    "vue-line-clamp": "^1.3.2",
    "vue-router": "^3.1.5",
    "vue-youtube": "^1.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-eslint": "^10.0.1",
    "bulma": "^0.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
